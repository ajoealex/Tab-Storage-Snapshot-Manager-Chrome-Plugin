{
    "manifest_version": 3,
    "name": "Tab Storage Snapshot Manager",
    "version": "1.0",
    "permissions": [
        "scripting",
        "activeTab",
        "storage",
        "cookies"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}